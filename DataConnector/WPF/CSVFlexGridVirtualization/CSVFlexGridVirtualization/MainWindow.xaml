<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CSVFlexGridVirtualization"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:c1="http://schemas.componentone.com/winfx/2006/xaml" x:Class="CSVFlexGridVirtualization.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <c1:FlexGrid x:Name="flexGrid">
            <i:Interaction.Behaviors>
                <c1:SkeletonLoadingBehavior />
            </i:Interaction.Behaviors>
        </c1:FlexGrid>

    </Grid>
</Window>
